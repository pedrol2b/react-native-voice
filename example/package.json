{
  "name": "example",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --no-dev --minify",
    "dev": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "expo prebuild --clean --skip-dependency-update react-native",
    "android:clean": "cd android && ./gradlew clean",
    "android:clean:cache": "cd android && rm -rf .gradle && rm -rf build && rm -rf app/build",
    "android:clean:all": "yarn android:clean && yarn android:clean:cache",
    "android:assemble:debug": "cd android && ./gradlew assembleDebug",
    "test": "jest --watchAll",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:typescript": "tsc --noEmit",
    "test:ci": "jest --runInBand --passWithNoTests --coverage",
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint --fix \"src/**/*\"",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\""
  },
  "author": "Pedro Bueno <pedrolbb.e@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@pedrol2b/react-native-voice": "^3.2.8",
    "expo": "~50.0.14",
    "expo-status-bar": "~1.11.1",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-native": "^0.73.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@react-native/babel-preset": "^0.75.0-main",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@types/jest@types/jest": "^29.5.12",
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.74",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jest-dom": "^5.2.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  },
  "engines": {
    "node": ">=18"
  },
  "private": false,
  "repository": "https://github.com/pedrol2b/react-native-voice.git"
}
